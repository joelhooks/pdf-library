{
  "name": "pdf-brain",
  "version": "0.3.1",
  "description": "Local PDF knowledge base with vector search",
  "type": "module",
  "main": "src/index.ts",
  "bin": {
    "pdf-brain": "./src/cli.ts"
  },
  "scripts": {
    "build": "bun build src/index.ts src/cli.ts --outdir dist --target bun",
    "dev": "bun run src/cli.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "setup": "./scripts/setup.sh"
  },
  "dependencies": {
    "@effect/platform": "^0.72.0",
    "@effect/platform-bun": "^0.52.0",
    "@effect/schema": "^0.75.0",
    "@electric-sql/pglite": "^0.3.14",
    "@electric-sql/pglite-tools": "^0.2.19",
    "effect": "^3.12.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "engines": {
    "bun": ">=1.0.0"
  },
  "files": [
    "src",
    "scripts"
  ],
  "keywords": [
    "pdf",
    "vector-search",
    "embeddings",
    "pglite",
    "pgvector",
    "knowledge-base",
    "effect-ts"
  ],
  "author": "Joel Hooks",
  "license": "MIT"
}
